
<div class="container new-page">
  <div class="row">
    <p>新しく投稿する</p>

    <%= form_with model: @note, local: true do |f| %>
      <div class="title-area">
        <%= f.label :title, "タイトル" %><br/>
        <%= f.text_field :title %>
      </div>
  </div>
</div>

<div class="content-back">
  <div class="content-field">
    <%= f.label :content, "本文" %>
    <%= f.text_area :content %>
  </div>
</div>

<% end %>